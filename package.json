{
  "name": "@point-hub/papp",
  "version": "0.0.146",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.umd.cjs"
      }
    },
    "./index.css": "./dist/index.css"
  },
  "scripts": {
    "dev": "run-s clean-dist build:lib && run-p build:watch docs:dev",
    "dev:src": "vite dev",
    "build": "run-s clean-dist build:lib docs:build",
    "build:lib": "vite build",
    "build:watch": "chokidar 'src/**/*' -c 'vite build' --initial",
    "test": "vitest",
    "test:e2e": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "types": "vue-tsc --build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean-dist": "./node_modules/rimraf/dist/esm/bin.mjs dist",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@point-hub/js-utils": "^0.0.6",
    "@point-hub/preset-papp-icon": "^0.0.12",
    "@tsconfig/node22": "^22.0.5",
    "@unocss/vite": "^66.5.10",
    "@vueuse/components": "^14.1.0",
    "@vueuse/core": "^14.1.0",
    "cleave.js": "^1.6.0",
    "date-fns": "^4.1.0",
    "pinia": "^3.0.4",
    "textarea-caret": "^3.1.0",
    "uuid": "^13.0.0",
    "vue-router": "4",
    "vue-tippy": "^6.7.1",
    "vue3-popper": "^1.5.0"
  },
  "peerDependencies": {
    "vue": "^3.5.14"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^24.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.8",
    "@types/cleave.js": "^1.4.12",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.0.2",
    "@types/textarea-caret": "^3.0.4",
    "@types/uuid": "^11.0.0",
    "@unocss/preset-web-fonts": "^66.5.10",
    "@unocss/reset": "^66.5.10",
    "@unocss/transformer-directives": "^66.5.10",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/eslint-plugin": "^1.5.2",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "ajv": "^8.17.1",
    "chokidar-cli": "^3.0.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "9.39.2",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.6.2",
    "jsdom": "^27.3.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.6",
    "rimraf": "^6.1.2",
    "start-server-and-test": "^2.1.3",
    "typescript": "5.9.3",
    "unocss": "^66.5.10",
    "vite": "^7.3.0",
    "vite-plugin-dts": "4.5.4",
    "vitepress": "^2.0.0-alpha.15",
    "vitest": "^4.0.15",
    "vue": "^3.5.25",
    "vue-tsc": "^3.1.8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/point-hub/papp.git"
  },
  "publishConfig": {
    "access": "public"
  }
}