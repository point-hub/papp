{
  "name": "@point-hub/papp",
  "version": "0.0.87",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.umd.cjs"
      }
    },
    "./index.css": "./dist/index.css"
  },
  "scripts": {
    "dev": "run-s clean-dist build:lib && run-p dev:lib docs:dev",
    "dev:src": "vite dev",
    "dev:lib": "vite build --watch",
    "build": "run-s clean-dist build:lib docs:build",
    "build:lib": "vite build",
    "test": "vitest",
    "types": "vue-tsc --build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean-dist": "./node_modules/rimraf/dist/esm/bin.mjs dist",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@point-hub/js-utils": "^0.0.5",
    "@point-hub/preset-papp-icon": "^0.0.2",
    "@vueuse/components": "^12.0.0",
    "@vueuse/core": "^12.0.0",
    "cleave.js": "^1.6.0",
    "date-fns": "^4.1.0",
    "pinia": "^2.2.6",
    "uuid": "^11.0.3",
    "vue": "^3.5.13",
    "vue-router": "4",
    "vue-tippy": "^6.5.0",
    "vue3-popper": "^1.5.0"
  },
  "devDependencies": {
    "@tsconfig/node20": "^20.1.4",
    "@types/cleave.js": "^1.4.12",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.10.0",
    "@types/uuid": "^10.0.0",
    "@unocss/preset-web-fonts": "^0.60.4",
    "@unocss/reset": "^0.60.4",
    "@unocss/transformer-directives": "^0.60.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/eslint-plugin": "^1.1.11",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.6.0",
    "ajv": "^8.17.1",
    "eslint": "9.14.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^9.31.0",
    "jsdom": "^25.0.1",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.1",
    "rimraf": "^6.0.1",
    "typescript": "5.7.2",
    "unocss": "^0.60.4",
    "vite": "^6.0.1",
    "vite-plugin-dts": "3.7.3",
    "vitepress": "1.5.0",
    "vitest": "^2.1.6",
    "vue-tsc": "^2.1.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/point-hub/papp.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
