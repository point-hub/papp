{
  "name": "@point-hub/papp",
  "version": "0.0.7",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.umd.cjs"
      }
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "run-s clean-dist build:lib && run-p dev:lib docs:dev",
    "dev:src": "vite dev",
    "dev:lib": "vite build --watch",
    "build": "run-s clean-dist build:lib docs:build",
    "build:lib": "vite build",
    "test": "vitest",
    "check-types": "vue-tsc --build",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "bun run lint -- --fix",
    "clean-dist": "./node_modules/rimraf/dist/esm/bin.mjs dist",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@point-hub/preset-papp-icon": "^0.0.2",
    "@unocss/reset": "^0.58.0",
    "cleave.js": "^1.6.0",
    "dayjs": "^1.11.10",
    "pinia": "^2.1.7",
    "uuid": "^9.0.1",
    "vue": "^3.3.11",
    "vue-router": "4",
    "vue-tippy": "^6.3.1",
    "vue3-popper": "^1.5.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.6.0",
    "@tsconfig/node18": "^18.2.2",
    "@types/cleave.js": "^1.4.12",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "@unocss/transformer-directives": "^0.58.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.3",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.55.0",
    "eslint-plugin-vue": "^9.19.2",
    "jsdom": "^23.0.1",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "unocss": "^0.58.0",
    "vite": "^5.0.7",
    "vite-plugin-dts": "^3.6.4",
    "vitepress": "^1.0.0-rc.31",
    "vitest": "^1.0.4",
    "vue-tsc": "^1.8.25"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/point-hub/papp.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
