{
  "name": "@point-hub/papp",
  "version": "0.0.117",
  "type": "module",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.umd.cjs"
      }
    },
    "./index.css": "./dist/index.css"
  },
  "scripts": {
    "dev": "run-s clean-dist build:lib && run-p dev:lib docs:dev",
    "dev:src": "vite dev",
    "dev:lib": "vite build --watch",
    "build": "run-s clean-dist build:lib docs:build",
    "build:lib": "vite build",
    "test": "vitest",
    "test:e2e": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "types": "vue-tsc --build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "clean-dist": "./node_modules/rimraf/dist/esm/bin.mjs dist",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@point-hub/js-utils": "^0.0.6",
    "@point-hub/preset-papp-icon": "^0.0.3",
    "@vueuse/components": "^13.2.0",
    "@vueuse/core": "^13.2.0",
    "cleave.js": "^1.6.0",
    "date-fns": "^4.1.0",
    "pinia": "^3.0.2",
    "uuid": "^11.1.0",
    "vue-router": "4",
    "vue-tippy": "^6.7.1",
    "vue3-popper": "^1.5.0"
  },
  "peerDependencies": {
    "vue": "^3.5.14"
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^22.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.5",
    "@tsconfig/node20": "^20.1.5",
    "@types/cleave.js": "^1.4.12",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.15.21",
    "@types/uuid": "^10.0.0",
    "@unocss/preset-web-fonts": "^0.60.4",
    "@unocss/reset": "^0.60.4",
    "@unocss/transformer-directives": "^0.60.4",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitest/eslint-plugin": "^1.2.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "ajv": "^8.17.1",
    "cypress-file-upload": "^5.0.8",
    "eslint": "9.27.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.1.0",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "start-server-and-test": "^2.0.12",
    "typescript": "5.8.3",
    "unocss": "^0.60.4",
    "vite": "^6.3.5",
    "vite-plugin-dts": "4.5.4",
    "vitepress": "1.6.3",
    "vitest": "^3.1.4",
    "vue": "^3.5.14",
    "vue-tsc": "^2.2.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/point-hub/papp.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
