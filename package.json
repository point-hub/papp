{
  "name": "@point-hub/papp",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/papp.umd.cjs",
  "module": "./dist/papp.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/papp.js"
      },
      "require": {
        "types": "./dist/src/index.d.ts",
        "default": "./dist/papp.umd.cjs"
      }
    },
    "./style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite dev",
    "dev:all": "run-s clean-dist && run-p dev:lib dev:type docs:dev",
    "dev:type": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json --watch",
    "dev:lib": "vite build --watch",
    "build": "run-s clean-dist build:type build:lib docs:build",
    "build:type": "vue-tsc --declaration --emitDeclarationOnly -p tsconfig.build.json",
    "build:lib": "vite build",
    "test": "vitest",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "bun run lint -- --fix",
    "clean-dist": "./node_modules/rimraf/dist/esm/bin.mjs dist",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.16",
    "@point-hub/preset-papp-icon": "^0.0.2",
    "@unocss/reset": "^0.57.2",
    "cleave.js": "^1.6.0",
    "dayjs": "^1.11.10",
    "pinia": "^2.1.7",
    "uuid": "^9.0.1",
    "vue": "^3.3.4",
    "vue-router": "4",
    "vue-tippy": "^6.3.1",
    "vue3-popper": "^1.5.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/cleave.js": "^1.4.12",
    "@types/jsdom": "^21.1.3",
    "@types/node": "^18.18.5",
    "@types/uuid": "^9.0.7",
    "@unocss/transformer-directives": "^0.57.2",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.1.1",
    "postcss": "^8.4.32",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "typescript": "~5.2.0",
    "unocss": "^0.57.2",
    "vite": "^4.4.11",
    "vitepress": "^1.0.0-rc.25",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/point-hub/papp.git"
  },
  "publishConfig": {
    "access": "public"
  }
}
